<Patch>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>boomer.Rebalance</modId>
		<key>pregnancyDuration</key>
		<defaultValue>18</defaultValue>
		<apply>
			<Operation Class="PatchOperationReplace">
				<xpath>//ThingDef[defName="Human"]/race/gestationPeriodDays/text()</xpath>
				<value>{pregnancyDuration}</value>
			</Operation>
		</apply>
	</Operation>
</Patch>