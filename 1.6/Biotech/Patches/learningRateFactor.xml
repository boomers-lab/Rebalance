<Patch>
	<Operation Class="XmlExtensions.UseSetting">
		<modId>boomer.Rebalance</modId>
		<key>learningRateFactor</key>
		<defaultValue>1</defaultValue>
		<apply>
			<Operation Class="PatchOperationReplace">
				<xpath>//StatDef[defName="LearningRateFactor"]/defaultBaseValue/text()</xpath>
				<value>{learningRateFactor}</value>
			</Operation>
		</apply>
	</Operation>
</Patch>