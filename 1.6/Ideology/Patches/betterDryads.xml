<Patch>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>boomer.Rebalance</modId>
		<key>betterDryads</key>
		<defaultValue>false</defaultValue>
		<caseTrue>
            <li Class="PatchOperationReplace">
                <xpath>//ThingDef[defName="Dryad_Woodmaker"]/comps/li/spawnCount/text()</xpath>
                <value>64</value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>//ThingDef[defName="Dryad_Berrymaker"]/comps/li/spawnCount/text()</xpath>
                <value>80</value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>//ThingDef[defName="Dryad_Medicinemaker"]/comps/li/spawnCount/text()</xpath>
                <value>8</value>
            </li>
        </caseTrue>
    </Operation>
</Patch>