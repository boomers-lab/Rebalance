<Patch>
	<Operation Class="XmlExtensions.OptionalPatch">
		<modId>boomer.Rebalance</modId>
		<key>lessMarshyBorealForest</key>
		<defaultValue>false</defaultValue>
		<caseTrue>
			<Operation Class="PatchOperationReplace">
				<xpath>//BiomeDef[defName="BorealForest"]/terrainPatchMakers/li/thresholds</xpath>
				<value>
					<thresholds>
						<li>
							<terrain>MossyTerrain</terrain>
							<min>0.73</min>
							<max>0.80</max>
						</li>
						<li>
							<terrain>Marsh</terrain>
							<min>0.80</min>
							<max>0.93</max>
						</li>
						<li>
							<terrain>WaterShallow</terrain>
							<min>0.93</min>
							<max>999</max>
						</li>
					</thresholds>
				</value>
			</Operation>
		</caseTrue>
	</Operation>
</Patch>